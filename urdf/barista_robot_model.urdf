        <visual>
            <geometry>
                <cylinder radius="0.178" length="0.155"/>
            </geometry>
            <material name="green_dark"/>
        </visual>

        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.178" length="0.155"/>
            </geometry>
        </collision>

        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="2.35"/>
            <inertia ixx="0.01392573958333333" ixy="0" ixz="0" iyy="0.01392573958333333" iyz="0" izz="0.02650260416666667"/>
        </inertial>
    </link>


