<?xml version="1.0"?>
<robot name="box_bot">

    <material name="green_dark">
        <color rgba="0.0 0.5 0.0 1"/>
    </material>

    <material name="gray">
        <color rgba="0.5 0.5 0.5 1.0"/> 
    </material>

    <material name="black">
        <color rgba="0.0 0.0 0.0 1"/>
    </material>

    <!-- main body -->

     <link name="base_link">
        <visual>
            <geometry>
                <cylinder radius="0.178" length="0.155"/>
            </geometry>
            <material name="green_dark"/>
        </visual>
    </link>


    <!-- Left Wheel -->

    <link name="left_wheel">
    <visual>
        <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
        <geometry>
        <cylinder length="0.0206" radius="0.0352"/>
        </geometry>
        <material name="black"/>
    </visual>
    </link>

    <joint name="joint_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 0.128 -0.0775"/>
        <child link="left_wheel"/>
        <parent link="base_link"/>
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>

    <!-- Right Wheel -->

    <link name="right_wheel">
    <visual>
        <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
        <geometry>
        <cylinder length="0.0206" radius="0.0352"/>
        </geometry>
        <material name="black"/>
    </visual>
    </link>

    <joint name="joint_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 -0.128 -0.0775"/>
        <child link="right_wheel"/>
        <parent link="base_link"/>
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>


   <!-- Standoff Rod 1 -->

    <link name="standoff_rod_1">
    <visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
        <cylinder length="0.220" radius="0.010"/>
        </geometry>
        <material name="gray"/>
    </visual>
    </link>

    <joint name="joint_standoff_rod_1" type="fixed">
        <origin rpy="0 0 0" xyz="0.100 0.100 0.1875"/>
        <parent link="base_link"/>
        <child link="standoff_rod_1"/>
    </joint>

    <!-- Standoff Rod 2 -->

    <link name="standoff_rod_2">
    <visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
        <cylinder length="0.220" radius="0.010"/>
        </geometry>
        <material name="gray"/>
    </visual>
    </link>

    <joint name="joint_standoff_rod_2" type="fixed">
        <origin rpy="0 0 0" xyz="-0.100 0.100 0.1875"/>
        <parent link="base_link"/>
        <child link="standoff_rod_2"/>
    </joint>

    <!-- Standoff Rod 3 -->

    <link name="standoff_rod_3">
    <visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
        <cylinder length="0.220" radius="0.010"/>
        </geometry>
        <material name="gray"/>
    </visual>
    </link>

    <joint name="joint_standoff_rod_3" type="fixed">
        <origin rpy="0 0 0" xyz="-0.100 -0.100 0.1875"/>
        <parent link="base_link"/>
        <child link="standoff_rod_3"/>
    </joint>

    <!-- Standoff Rod 4 -->

    <link name="standoff_rod_4">
    <visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
        <cylinder length="0.220" radius="0.010"/>
        </geometry>
        <material name="gray"/>
    </visual>
    </link>

    <joint name="joint_standoff_rod_4" type="fixed">
        <origin rpy="0 0 0" xyz="0.100 -0.100 0.1875"/>
        <parent link="base_link"/>
        <child link="standoff_rod_4"/>
    </joint>


    <!-- Top Base -->

    <link name="top_base">
    <visual>
        <geometry>
        <cylinder radius="0.160" length="0.090"/>
        </geometry>
        <material name="green_dark"/>
    </visual>
    </link>

    <joint name="joint_top_base" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 0.3425"/>
        <parent link="base_link"/>
        <child link="top_base"/>
    </joint>

</robot>